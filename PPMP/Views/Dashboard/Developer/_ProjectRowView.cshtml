@model ProjectRowViewModel

<tr class="hover:bg-gray-50 transition-colors">
    <td class="px-4 py-3 text-left text-[13px] font-medium text-gray-700">@Model.Name</td>

    <td class="px-4 py-3 text-left text-[13px] font-normal text-gray-700">
        @Model.ClientName
    </td>

    <td class="px-4 py-3 text-left text-[13px] font-normal text-gray-700">
        @Model.state
    </td>

    <!-- TODO: This Is PCR UI Its Meant to Be A Bar Graphic Showing A Percentage -->
    <td class="px-4 py-3 text-left text-[13px] font-normal text-gray-700">
       @Model.ProgressRate
    </td>
</tr>